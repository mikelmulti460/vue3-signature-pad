{
  "name": "@selemondev/vue3-signature-pad",
  "type": "module",
  "version": "1.3.1",
  "private": false,
  "packageManager": "pnpm@9.15.2",
  "description": "Vue 3 based smooth signature drawing component âœ¨",
  "author": "Selemondev",
  "homepage": "https://github.com/selemondev/vue3-signature-pad#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selemondev/vue3-signature-pad.git"
  },
  "bugs": {
    "url": "https://github.com/selemondev/vue3-signature-pad/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "dev": "tsup --watch --onSuccess \"pnpm build:dts && pnpm --filter website dev\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate:release": "npx changelogen@latest --release && npm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@selemondev/vue3-signature-pad": "^1.3.0",
    "nanoid": "^5.0.9",
    "signature_pad": "^5.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2",
    "unplugin-vue": "^5.2.1",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
